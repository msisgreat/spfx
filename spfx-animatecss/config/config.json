{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "spfx-animate-wp-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/spfxAnimateWp/SpfxAnimateWpWebPart.js",
          "manifest": "./src/webparts/spfxAnimateWp/SpfxAnimateWpWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {
    "jquery": {
      "path": "node_modules/jquery/dist/jquery.min.js",
      "globalName": "jQuery"
    },
    "bootstrap": {
      "path": "node_modules/bootstrap/dist/js/bootstrap.bundle.min.js",
      "globalName": "bootstrap",
      "globalDependencies": [
        "jquery"
      ]
    }
  },
  "localizedResources": {
    "SpfxAnimateWpWebPartStrings": "lib/webparts/spfxAnimateWp/loc/{locale}.js"
  }
}
