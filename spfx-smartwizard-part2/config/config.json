{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "spfx-smartwizard-2-wp-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/spfxSmartwizard2Wp/SpfxSmartwizard2WpWebPart.js",
          "manifest": "./src/webparts/spfxSmartwizard2Wp/SpfxSmartwizard2WpWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {
    "jquery": {
      "path": "node_modules/jquery/dist/jquery.min.js",
      "globalName": "jQuery"
    },
    "bootstrap": {
      "path": "node_modules/bootstrap/dist/js/bootstrap.bundle.min.js",
      "globalName": "bootstrap",
      "globalDependencies": [
        "jquery"
      ]
    },
    "smartwizrd": {
      "path": "node_modules/smatwizard/dist/js/jquery.smartWizard.min.js",
      "globalName": "smartwizrd"
    }
  },
  "localizedResources": {
    "SpfxSmartwizard2WpWebPartStrings": "lib/webparts/spfxSmartwizard2Wp/loc/{locale}.js"
  }
}
